<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pergunta5Fragment" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="5. Você teve contato com pessoas que tiveram teste positivo ou suspeito nos últimos 14 dias? "
            android:textColor="#000000" />

        <RadioGroup
            android:id="@+id/radioGroupP5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RadioButton
                android:id="@+id/radioButton5a"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Sim e ainda não fiz o teste para COVID-19 ou aguardo resultado / Sim, já fiz o teste e o resultado foi positivo para COVID-19  " />

            <RadioButton
                android:id="@+id/radioButton5b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Sim, já fiz o teste e o resultado foi negativo para COVID-19 / Não / não sei" />

        </RadioGroup>

        <Button
            android:id="@+id/buttonPergunta5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/textProximo" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>